{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "critical",
      "resources": {
        "files": ["/**/*.css", "/**/*.js"]
      }
    },
    {
      "name": "icons",
      "resources": {
        "files": ["/icons/**"],
        "cacheConfig": { "strategy": "freshness", "maxSize": 20 }
      }
    }
  ],
  "dataGroups": [
    {
      "name": "strategy-data",
      "urls": [
        "/blackjack_strategy.json"
      ],
      "cacheConfig": {
        "maxSize": 1,
        "maxAge": "7d",
        "strategy": "freshness",
        "timeout": "3s"
      }
    }
  ]
}
