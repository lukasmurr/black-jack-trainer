{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/game-cards/**/*",
          "/icons/**/*",
          "/**/*.json",
          "/**/*.svg",
          "/**/*.png"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "strategy-data",
      "urls": [
        "/blackjack_strategy.json"
      ],
      "cacheConfig": {
        "maxSize": 1,
        "maxAge": "7d",
        "strategy": "freshness",
        "timeout": "3s"
      }
    }
  ]
}
